{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "flutter: build widgetbook",
			"type": "flutter",
			"command": "flutter",
			"args": [
				"build",
				"web",
				"--wasm",
				"--base-href",
				"/widgetbook/",
				"--pwa-strategy",
				"none",
				"--dart-define",
				"IS_IMPORTED=true",
				"--dart-define",
				"SERVER_NAME=https://dev.tentura.io",
				"--dart-define",
				"IMAGE_SERVER=https://static.tentura.io",
			],
			"group": {
				"kind": "build",
			},
		},
	],
}