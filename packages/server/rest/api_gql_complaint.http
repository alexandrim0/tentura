#
### GQL Create Complaint
#
POST {{base_url}}/{{gqlV2_path}}
Authorization: Bearer {{$dotenv TENTURA_JWT}}
Content-Type: application/json
X-Request-Type: GraphQL

mutation CreateComplaint(
  $id: String!,
  $type: String!,
  $email: String!,
  $details: String!
) {
  complaintCreate(
    id: $id,
    type: $type,
    email: $email,
    details: $details
  )
}

